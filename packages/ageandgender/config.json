{
	"$schema": "https://raw.githubusercontent.com/arsenstorm/request-directory/main/schema.json",
	"name": "Age and Gender",
	"slug": "ageandgender",
	"description": "Detect the age and gender of a person in an image.",
	"port": 7003,
	"pricing": {
		"type": "fixed",
		"price": 0.01
	},
	"tag": "Vision",
	"api": {
